{
  "name": "gameon",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "firebase:emulators": "firebase emulators:start",
    "firebase:deploy": "firebase deploy",
    "firebase:functions:build": "cd functions && npm run build",
    "seed": "tsx scripts/seed.ts",
    "seed:small": "tsx scripts/seed.ts --scale small",
    "seed:medium": "tsx scripts/seed.ts --scale medium",
    "seed:large": "tsx scripts/seed.ts --scale large --stream",
    "seed:xl": "SEED=42 tsx scripts/seed.ts --scale xl --stream --verbose"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "dayjs": "^1.11.13",
    "firebase": "^10.7.1",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-swipeable": "^7.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "commander": "^11.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.20.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
